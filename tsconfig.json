{
  "compilerOptions": {
    "target": "ESNext",
    "module": "preserve",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "jsx" :"react-jsx"
  },
  "include": ["server.ts", "**/*.ts", "**/*.tsx"]
}
